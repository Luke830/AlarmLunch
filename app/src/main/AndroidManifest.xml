            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <receiver android:name=".AlarmRecievier" />

        <activity
            android:name=".ShowActivity"
            android:theme="@style/Theme.Transparent">


